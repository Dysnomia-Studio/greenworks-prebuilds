{
  "name": "greenworks-prebuilds",
  "version": "0.21.0",
  "description": "Prebuild binaries for greenworks",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "zip-sdk": "node scripts/zipSdk.js",
    "watch": "tsc -w",
    "build": "tsc",
    "dev": "yarn run dev:node",
    "dev:node": "node dist/index.js --os=ubuntu-latest --runtime=node --arch=x64",
    "dev:nwjs": "node dist/index.js --os=ubuntu-latest --runtime=nw.js --arch=x64",
    "dev:electron": "node dist/index.js --os=ubuntu-latest --runtime=electron --arch=x64",
    "lint": "eslint ."
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Dysnomia-Studio/greenworks-prebuilds/blob/main/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Dysnomia-Studio/greenworks-prebuilds.git"
  },
  "dependencies": {
    "@electron/get": "^4.0.2",
    "dotenv": "^17.2.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "got": "^14.4.7",
    "modules-abi": "^1.2.0",
    "mri": "^1.2.0",
    "node-abi": "^4.12.0",
    "node-gyp": "^11.2.0",
    "nw-gyp": "^3.6.8",
    "tar": "^7.4.3",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/mri": "^1.2.0",
    "@types/node": "^24.1.0",
    "@types/source-map-support": "^0.5.10",
    "@types/tar": "^6.1.13",
    "@types/unzipper": "^0.10.11",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "source-map-support": "^0.5.21",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
